{
    "ignoreMsiWithNoMutations": true,
    "logs": {
        "github": false,
        "html": "build/infection/infection.html",
        "text": "build/infection/infection.text"
    },
    "minCoveredMsi": 80,
    "minMsi": 80,
    "mutators": {
        "@default": true,
        "@function_signature": true,
        "DecrementInteger": {
            "ignoreSourceCodeByRegex": [
                ".*"
            ]
        },
        "global-ignore": [
            "Client::__construct"
        ],
        "global-ignoreSourceCodeByRegex": [
            "Assert::.*"
        ],
        "IncrementInteger": {
            "ignore": [
                "Guanguans\\Notify\\ServerChan\\Messages\\Message::encrypt",
                "Guanguans\\Notify\\Foundation\\Concerns\\Dumpable::dd"
            ],
            "ignoreSourceCodeByRegex": [
                ".*"
            ]
        }
    },
    "phpUnit": {
        "configDir": "./"
    },
    "source": {
        "directories": [
            "src"
        ],
        "excludes": [
            "Foundation/Rectors",
            "Foundation/Support"
        ]
    },
    "testFramework": "pest",
    "timeout": 10
}